.item-wrap{
  margin: 40rpx 20rpx
}
.item-wrap .institution-name {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 10rpx
}
.item-wrap .body {
  border-radius: 10rpx;
  border: 1px solid #999;
  overflow: hidden;
}
.item-wrap .body .img {
  width: 100%;
  float: left;
}
.item-wrap .body .info {
  padding: 10rpx 20rpx;
  clear: both
}
.item-wrap .body .info .title {
  font-size: 40rpx;
}
@import "../common.wxss";

