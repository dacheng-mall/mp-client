.item-wrap{
  margin: 32rpx
}
.item-wrap .institution-name {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 10rpx
}
.item-wrap .body {
  border-radius: 10rpx;
  border: 1px solid #999;
  overflow: hidden;
}
.item-wrap .body .img {
  width: 100%;
  float: left;
}
.item-wrap .body .info {
  padding: 10rpx 20rpx;
  clear: both
}
.item-wrap .body .info .title {
  font-size: 40rpx;
}
/* @import "../common.wxss"; */

.info .range {
  font-size: 24rpx;
  color: #666
}
.free-text {
  padding: 10px;
  text-align: center;
  font-size: 44rpx;
  background: #00bcbd;
  color: #fff;
  font-weight: bold;
}